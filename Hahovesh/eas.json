{
  "cli": {
    "version": ">= 16.10.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
    "autoIncrement": true,
    "env": {
      "GOOGLE_SERVICES_JSON": "@GOOGLE_SERVICES_JSON"
    },
    "android": {
      "buildType": "apk",
      "gradleCommand": ":app:assembleRelease"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
